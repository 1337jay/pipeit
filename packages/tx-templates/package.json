{
  "name": "@pipeit/tx-templates",
  "version": "0.1.0",
  "description": "Pre-built transaction templates for common Solana operations",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js",
      "require": "./dist/core/index.cjs"
    },
    "./token": {
      "types": "./dist/token/index.d.ts",
      "import": "./dist/token/index.js",
      "require": "./dist/token/index.cjs"
    },
    "./nft": {
      "types": "./dist/nft/index.d.ts",
      "import": "./dist/nft/index.js",
      "require": "./dist/nft/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "keywords": [
    "solana",
    "transaction",
    "templates",
    "tokens",
    "nft"
  ],
  "license": "MIT",
  "peerDependencies": {
    "gill": "^0.12.0"
  },
  "dependencies": {
    "@pipeit/tx-core": "workspace:*",
    "@pipeit/tx-errors": "workspace:*"
  },
  "devDependencies": {
    "gill": "^0.12.0",
    "@types/node": "^24",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}

